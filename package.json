{
  "name": "dnctopics-nuxt",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Sean Washington <hello@seanwash.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./server/index.js --watch server --watch nuxt.config.js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node ./server/index.js",
    "generate": "nuxt generate",
    "preview": "browser-sync start --server --ss dist --files dist/index.html",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "engines": {
    "node": "10.5.0",
    "npm": "6.1.0"
  },
  "dependencies": {
    "@nuxtjs/axios": "^3.1.4",
    "@nuxtjs/markdownit": "^1.2.1",
    "@nuxtjs/sitemap": "^0.1.1",
    "date-fns": "^1.29.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "lunr": "^2.3.0",
    "morgan": "^1.9.0",
    "nuxt": "^1.4.0",
    "tachyons": "^4.9.1",
    "vuex": "^3.0.1",
    "cross-env": "^5.1.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "browser-sync": "^2.24.4",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "nodemon": "^1.17.5"
  }
}
